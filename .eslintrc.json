{
    "env": {
        "browser": true,
        "es2021": true
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "script"
    },
    "rules": {
        "no-console": "off",
        "no-undef": "error",
        "no-unused-vars": "warn",
        "prefer-const": "warn",
        "no-var": "error"
    }
}
